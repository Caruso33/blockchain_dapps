{
  "name": "multiavatar",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "compile": "hardhat compile",
    "create:avatars": "node scripts/createAvatars",
    "upload:files": "node scripts/uploadToIpfs.js",
    "create:marketNfts": "hardhat run scripts/createMarketNfts.js",
    "run:all:local": "yarn run compile && yarn run create:avatars --numberOfAvatars 2 && yarn run upload:files && yarn create:marketNfts --price 0.01 --network localhost",
    "run:all:mumbai": "yarn run compile && yarn run create:avatars --numberOfAvatars 2 && yarn run upload:files && yarn create:marketNfts --price 0.01 --network mumbai"
  },
  "dependencies": {
    "@faker-js/faker": "^7.2.0",
    "@multiavatar/multiavatar": "^1.0.7",
    "@openzeppelin/contracts": "^4.6.0",
    "@pinata/sdk": "^1.1.26",
    "dotenv": "^16.0.1",
    "hardhat": "^2.9.9",
    "ipfs-http-client": "^57.0.1",
    "svg2img": "^1.0.0-beta.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0"
  }
}
